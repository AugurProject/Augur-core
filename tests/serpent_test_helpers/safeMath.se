inset('../../src/macros/safeMath.sem')

def add(x, y):
    return safeAdd(x, y)

def subtract(x, y):
    return safeSub(x, y)

def multiply(x, y):
    return safeMul(x, y)

def fxpMultiply(x, y):
    return safeFxpMul(x, y)

def divide(x, y):
    return safeDiv(x, y)

def fxpDivide(x, y):
    return safeFxpDiv(x, y)

def fix(x):
    return fix(x)

def unfix(x):
    return unfix(x)
